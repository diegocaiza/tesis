	<queryString>
		<![CDATA[select distinct (al.AL_NOMBRES||' '||al.al_apellidos) as al_nombres,asis.ASIS_FECHA,
(asis.ASIS_ESTADO1 || '     ' ||asis.ASIS_ESTADO2) AS M__T,prom.PRO_NOMBRE,prog.PP_NOMBRE,para.PL_NOMBRE,
modu.MOD_NOMBRE
from alumno al,ASISTENCIA_ALUMNO asis,promocion prom,PROGRAMA_POSTGRADO prog,PARALELO para,
MODULO modu,FECHA_DIA fec
where al.AL_CEDULA=asis.AL_CEDULA and al.PROM_CODIGO=prom.PROM_CODIGO
and prog.PP_CODIGO=prom.PP_CODIGO and para.PL_CODIGO=prom.PL_CODIGO
and prog.PP_CODIGO=modu.PP_CODIGO and modu.MOD_CODIGO=fec.MOD_CODIGO and prom.PROM_CODIGO=fec.PROM_CODIGO
and prom.PROM_CODIGO=$P{cod_promocion} and to_char(asis.ASIS_FECHAREGISTRO,'yyyy-mm-dd') between $P{fechai} and $P{fechaf}]]>
	</queryString>
