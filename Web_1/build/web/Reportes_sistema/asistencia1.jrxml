	<queryString>
		<![CDATA[select  (al.AL_NOMBRES||' '||al.al_apellidos) as al_nombres,asis.ASIS_FECHA,
(asis.ASIS_ESTADO1 || '     ' ||asis.ASIS_ESTADO2) AS M__T,asis.ASIS_ESTADO1,asis.ASIS_ESTADO2,prom.PRO_NOMBRE,prog.PP_NOMBRE,para.PL_NOMBRE,modu.MOD_NOMBRE,
(prof.PR_NOMBRES||''||prof.pr_apellidos) as pr_nombres
from alumno al,ASISTENCIA_ALUMNO asis,PROMOCION prom,PROGRAMA_POSTGRADO prog,PARALELO para,FECHA_DIA fec,MODULO modu,
PROFESOR prof
where al.AL_CEDULA=asis.AL_CEDULA and al.PROM_CODIGO=prom.PROM_CODIGO and prog.PP_CODIGO=prom.PP_CODIGO
and prom.PL_CODIGO=para.PL_CODIGO and fec.MOD_CODIGO=modu.MOD_CODIGO 
and fec.PROM_CODIGO=prom.PROM_CODIGO and to_char(asis.asis_fECHAregistro,'yyyy-mm-dd') =to_char(fec.FD_FECHA,'yyyy-mm-dd') 
and prom.PROM_CODIGO=$P{cod_promocion} and to_char(asis.asis_fECHAregistro,'yyyy-mm-dd') between $P{fechai} and $P{fechaf}
and prof.PR_CEDULA=fec.PR_CEDULA]]>
	</queryString>
