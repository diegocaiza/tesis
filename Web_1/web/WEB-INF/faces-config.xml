<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>/Timbre.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/Asistencia_estudiante.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>No tiene un cronograma asignado!</from-outcome>
            <to-view-id>/Timbre.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    
    
    
       <navigation-rule>
        <description>Login</description>
        <from-view-id>/index1.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/inicio.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>incorrecto</from-outcome>
            <to-view-id>/index1.xhtml</to-view-id>
        </navigation-case>
        </navigation-rule>
    
    
       <navigation-rule>
        <from-view-id>/inicio.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrar</from-outcome>
            <to-view-id>/index1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    
    
    
    <navigation-rule>
        <description>Login</description>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/inicio.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>incorrecto</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/inicio.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrar</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
<navigation-case>
<from-outcome>cerrar</from-outcome>
<to-view-id>/index.xhtml</to-view-id>
</navigation-case>
</navigation-rule>
    
    
<application>
     <resource-bundle>
          <base-name>paq_mensajes.mensajes</base-name>
          <var>msg</var>
     </resource-bundle>
</application>
    <navigation-rule>
        <from-view-id>/Asistencia_estudiante.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>No tiene un cronograma asignado!</from-outcome>
            <to-view-id>/Asistencia_estudiante.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Error de credenciales</from-outcome>
            <to-view-id>/Asistencia_estudiante.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
