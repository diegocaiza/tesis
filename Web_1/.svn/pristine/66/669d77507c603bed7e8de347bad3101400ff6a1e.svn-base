	<queryString>
		<![CDATA[select distinct (prof.PR_NOMBRES||' '||prof.pr_apellidos) as nombres,prof.PR_CEDULA,
ti.T_ENTRADA2,ti.T_SALIDA2,((ti.T_SALIDA2-ti.T_ENTRADA2)*24) as total_horas,fec.FD_HORA_I as entrada_obligatoria,ti.t_fecha,
fec.FD_HORA_F as salida_obligatoria,prom.PRO_NOMBRE,pp.PP_NOMBRE,modu.MOD_NOMBRE,modu.MOD_NUMEROCREDITOS
from TIMBRE ti,PROFESOR prof,FECHA_DIA fec,MODULO modu,PROGRAMA_POSTGRADO pp,PROMOCION prom
where prof.PR_CEDULA=ti.PR_CEDULA and to_char(ti.T_FECHA,'yyyy-mm-dd') between  $P{fecha_i}and $P{fecha_f}
and prof.PR_CEDULA=fec.PR_CEDULA and fec.FD_FECHA=ti.T_FECHA
and modu.MOD_CODIGO=fec.MOD_CODIGO and pp.PP_CODIGO=modu.PP_CODIGO
and prom.PP_CODIGO=pp.PP_CODIGO
and prom.PROM_CODIGO=fec.PROM_CODIGO
and ti.pr_cedula=$P{cedula} order by ti.t_fecha asc]]>
	</queryString>
