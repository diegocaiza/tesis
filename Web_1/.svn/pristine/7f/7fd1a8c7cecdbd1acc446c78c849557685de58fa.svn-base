    public ArrayList<Car> getCars() {
        cars = new ArrayList<Car>();
        this.cars = lista();
        return cars;
    }

    private ArrayList<Car> lista() {
       ArrayList list = new ArrayList();
        crs = acciones.getLista();
        try {
            Car objeto;
            while (crs.next()) {
                objeto = new Car();
                objeto.setCodigo(crs.getString("ta_codigo"));
                objeto.setNombre(crs.getString("ta_nombre"));
                list.add(objeto);
            }
        } catch (Exception e) {
            System.out.println(e.getMessage());
        }
        return list;
    }
