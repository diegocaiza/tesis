                            <p:calendar value="#{vista_web_cronograma.fechaa}" navigator="true" yearRange="1905:c" pattern="yyyy-MM-dd"/>                              
                            <br></br>
                            <p:inputText id="aceptacionsenores" value="#{vista_web_cronograma.senores}"/>
                            <br></br>
                            <p:inputTextarea id="aceptacionnombre" rows="2" cols="100" value="#{vista_web_cronograma.nombre_ingenieroa}"/>
                            <br></br>
                            <p:inputTextarea id="aceptaciontitulo1" rows="2" cols="100" value="#{vista_web_cronograma.referencia}"/>  
                            <br></br>
