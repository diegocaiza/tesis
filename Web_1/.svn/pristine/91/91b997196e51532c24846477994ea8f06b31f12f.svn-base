    <ui:define name="content">

        <h:head>
            <title>CERTIFICADOS</title>
            <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
            <link href="estilos/jsfcrud.css" type="text/css" rel="stylesheet" />
        </h:head>
        <h:body>
            <h:form  id="fr"> 
                <p:messages id="messages" showDetail="true" autoUpdate="true" closable="true" />  
                <h:outputLabel value="Promoción:" /> 
                <br></br>
                <p:selectOneMenu id="lista1" value="#{vista_web_cronograma.txtpromocion}"  >
                    <f:selectItem itemLabel="Seleccione una promoción" itemValue="" />  
                    <f:selectItems value="#{vista_web_cronograma.listapromo}"/>
                    <p:ajax update="tabla"  
                            listener="#{vista_web_cronograma.devolvertabla}"/>
                </p:selectOneMenu>
                <p:dataTable emptyMessage="No existen registros" selectionMode="single" rowKey="#{lis.codigo}" paginator="true" rows="5" id="tabla" value="#{vista_web_cronograma.lista1}"  var="lis" filteredValue="#{vista_web_cronograma.filter}" selection="#{vista_web_cronograma.entidad}">
                    <p:ajax event="rowSelect" listener="#{vista_web_cronograma.seleccionLista}" update=":fr,@([id$=invitacion])"/>  
                    <p:ajax event="rowSelect" listener="#{vista_web_cronograma.seleccionLista_aceptacion}" update=":fr,@([id$=aceptacion])"/>  
                    <p:ajax event="rowSelect" listener="#{vista_web_cronograma.seleccionLista_personal}" update=":fr,@([id$=certi_personal])"/>  
                    <p:ajax event="rowSelect" listener="#{vista_web_cronograma.seleccioncontrato}" update=":fr,@([id$=contrato])"/>
                    <p:ajax event="rowSelect" listener="#{vista_web_cronograma.seleccion_informe_instructor}" update=":fr,@([id$=Informe])"/>
                    <p:ajax event="rowSelect" listener="#{vista_web_cronograma.seleccion_informe_cumplimiento_director}" update=":fr,@([id$=Informe_c])"/>
        
                    <p:column headerText="Estado">
                        <h:outputText value="#{lis.estado_fecha}"  style="font-size:20px"/>
                    </p:column>
                   
                    <p:column headerText="Módulo">

                        <h:outputText value="#{lis.nombre_mod}"/>
                    </p:column>
                    <p:column headerText="signatura o Unidad tematica">

                        <h:outputText value="#{lis.asig_codigo}"/>
                    </p:column>
                    <p:column headerText="Profesor">
                        <h:outputText value="#{lis.nombre_profesor}"/>
                    </p:column>
                    <p:column headerText="Hora Inicio">
                        <h:outputText value="#{lis.fd_hora_i}"/>
                    </p:column>

                    <p:column headerText="Fecha de clases">
                        <h:outputText value="#{lis.fd_fecha}" />
                    </p:column>

                </p:dataTable>

            </h:form> 
            <h:form  id="fr2"> 
                <p:tabView id="documentos" dynamic="true" cache="true">  
                    <p:tab title="Carta Invitación">  
                        <h:panelGrid columns="2" cellpadding="10" id="invitacion">  
                            <p:inputText id="numero_form" value="#{vista_web_cronograma.num_formulario}" size="20" />
                            <br></br>
                            <p:inputText id="titulo" value="#{vista_web_cronograma.titulo}" size="60"/>
                            <br></br>
                            <p:inputTextarea id="titulo1" rows="2" cols="100" value="#{vista_web_cronograma.titulo1}"/>  
                            <br></br>
                            <h:outputLabel  value="Fecha:" />
                            <br></br>
                            <p:calendar value="#{vista_web_cronograma.fecha_es}" navigator="true" yearRange="1905:c" pattern="yyyy-MM-dd" />                              
                            <br></br>
                            <p:inputTextarea id="nombre" rows="2" cols="100" value="#{vista_web_cronograma.nombre_ingeniero}"/>
                            <br></br>
                            <p:inputTextarea id="unidad_tematica" rows="1" cols="100" value="#{vista_web_cronograma.unidad_tematica}"/>
                            <br></br>
                            <p:inputTextarea id="asignatura" rows="1" cols="100" value="#{vista_web_cronograma.asignatura}" />
                            <br></br>
                            <p:inputTextarea id="programa" rows="1" cols="100" value="#{vista_web_cronograma.nombre_programa_invitacion}" />
                            <br></br>
                            <p:inputTextarea id="promocion" rows="1" cols="100" value="#{vista_web_cronograma.promocion}"/>
                            <br></br>
                            <p:inputTextarea id="fecha_clases" rows="1" cols="100" value="#{vista_web_cronograma.fecha_clases}"/>
                            <br></br>
                            <p:inputTextarea id="nota" rows="1" cols="100" value="#{vista_web_cronograma.nota}" />
                            <br></br>
                            <p:inputTextarea id="valor" rows="1" cols="100" value="#{vista_web_cronograma.valor}" />
                            <br></br>
                            <p:selectOneMenu id="#{msg.id_lista1}" value="#{vista_web_cronograma.director}">
                                <f:selectItem itemLabel="Remitente" itemValue="" />  
                                <f:selectItems value="#{vista_web_cronograma.listadirectores}"/>
                                <p:ajax update="remitente,invitacion"  
                                        listener="#{vista_web_cronograma.devolveremitente}" /> 
                            </p:selectOneMenu>
                            <br></br>
                            <h:outputLabel  value="Remitente" />
                            <br></br>
                            <p:inputTextarea id="remitente" rows="3" cols="100" value="#{vista_web_cronograma.remitente}" />
                        </h:panelGrid>  
                        <h:commandButton id="b2" value="imprimir" action="#{clase_reportes.reporte_carta_invitacion}" /> 

                    </p:tab>  
                    <p:tab title="Carta Aceptación">  
