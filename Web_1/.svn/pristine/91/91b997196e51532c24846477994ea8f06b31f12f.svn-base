                            <p:inputTextarea id="aceptacionremitente" rows="3" cols="100" value="#{vista_web_cronograma.remitentea}"/>
                        </h:panelGrid>  
                        <h:commandButton id="aceptacionb2" value="imprimir" action="#{clase_reportes.reporte_carta_aceptacion}" /> 
                    </p:tab>  
                    <p:tab title="Certificado Personal">  
                        <h:panelGrid columns="2" cellpadding="10" id="certi_personal">  
