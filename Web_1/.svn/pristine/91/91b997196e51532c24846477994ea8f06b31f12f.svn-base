                            <br></br>
                            <p:inputTextarea id="aceptacionasignatura" rows="1" cols="100" value="#{vista_web_cronograma.asignaturaa}"/>
                            <br></br>
                            <p:inputTextarea id="aceptacionprograma" rows="1" cols="100" value="#{vista_web_cronograma.programa_postgradoa}"/>
                            <br></br>
                            <p:inputTextarea id="aceptacionpromocion" rows="1" cols="100" value="#{vista_web_cronograma.promociona}"/>
                            <br></br>
                            <p:inputTextarea id="aceptacionfecha_clases" rows="1" cols="100" value="#{vista_web_cronograma.fecha_clasesa}"/>
                            <br></br>
                            <p:inputTextarea id="aceptacionvalor" rows="1" cols="100" value="#{vista_web_cronograma.valora}"/>
