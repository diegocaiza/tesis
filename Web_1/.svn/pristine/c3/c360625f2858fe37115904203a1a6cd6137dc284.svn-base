	<queryString>
		<![CDATA[select prog.PP_NOMBRE,pl.PL_NOMBRE,uni.UTEM_NOMBRE,uni.UTEM_NUMEROCREDITOS,
(prof.PR_NOMBRES||' '||prof.PR_APELLIDOS),modu.MOD_NOMBRE, modu.mod_numerocreditos,
au.AU_NUMERO,au.AU_BLOQUE,au.AU_PISO,
asig.ASIG_NOMBRE,asig.ASIG_NUMEROCREDITOS,
fdi.FD_HORA_I,fdi.FD_HORA_F,fdi.FD_FECHA,prom.pro_nombre
from profesor prof,promocion prom,FECHA_DIA fdi,aula au,modulo modu,ASIGNATURA asig,
PROGRAMA_POSTGRADO prog,PARALELO pl,UNIDAD_TEMATICA uni
where 
au.AU_CODIGO=fdi.AU_CODIGO  and 
prog.PP_CODIGO=modu.PP_CODIGO and
modu.MOD_CODIGO=asig.MOD_CODIGO  and
asig.ASIG_CODIGO=uni.ASIG_CODIGO and
uni.UTEM_CODIGO=fdi.UTEM_CODIGO and
prof.PR_CEDULA=fdi.PR_CEDULA and 
prog.PP_CODIGO=prom.PP_CODIGO and
pl.PL_CODIGO=prom.PL_CODIGO and
prom.PROM_CODIGO=$P{cod_promocion}
and to_char(fdi.FD_FECHA,'yyyy-mm-dd') between $P{fechai} and $P{fechaf}]]>
